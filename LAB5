let dept = ["is", "cs", "Mech"];
let str = "";

for (let x of dept) {
  str += x ;
}
console.log(str)
str+='abc'
for (let y in str) {
    console.log(str[y])
  }
str = str + Math.random()
console.log(str)
let str2={ name:"abc",dept:"ece"}
let str3=str+`${str2}`
console.log(str3);
let str4=str+str2.name;
